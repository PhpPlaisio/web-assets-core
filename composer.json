{
  "name": "plaisio/web-assets-core",
  "description": "PhpPlaisio: Core Web Assets",
  "keywords": [
    "PhpPlaisio",
    "Web Assets",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "plaisio/helper-html": "^4.1.0",
    "plaisio/helper-url": "^1.2.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/web-assets": "^3.1.0",
    "setbased/exception": "^2.3.0",
    "symfony/filesystem": "^6.2.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.3",
    "plaisio/console-kernel": "^1.0.0",
    "plaisio/dirs-core": "^1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\WebAssets\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\WebAssets\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
