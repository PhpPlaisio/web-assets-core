{
  "name": "plaisio/web-assets-core",
  "description": "PhpPlaisio: Core Web Assets",
  "keywords": [
    "PhpPlaisio",
    "Web Assets",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "plaisio/kernel": "^1.0",
    "plaisio/helper-html": "^1.0",
    "plaisio/helper-url": "^1.0",
    "plaisio/web-assets": "^1.0",
    "setbased/exception": "^2.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0",
    "plaisio/dirs-core": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\WebAssets\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\WebAssets\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
