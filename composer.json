{
  "name": "plaisio/web-assets-core",
  "description": "PhpPlaisio: Core Web Assets",
  "keywords": [
    "PhpPlaisio",
    "Web Assets",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "plaisio/helper-html": "^3.0.0",
    "plaisio/helper-url": "^1.1.2",
    "plaisio/kernel": "^3.1.1",
    "plaisio/web-assets": "^2.0.0",
    "setbased/exception": "^2.2.0",
    "webmozart/path-util": "^2.3.0"
  },
  "require-dev": {
    "phing/phing": "^2.17.0",
    "phpunit/phpunit": "^9.5.10",
    "plaisio/console": "^2.4.9",
    "plaisio/dirs-core": "^1.1.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\WebAssets\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\WebAssets\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
